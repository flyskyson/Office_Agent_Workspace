# 市场监管智能体 - 环境变量配置示例
# 复制此文件为 .env 并填写实际值

# 百度OCR API配置
# 获取地址: https://cloud.baidu.com/product/ocr
BAIDU_OCR_API_KEY=your_api_key_here
BAIDU_OCR_SECRET_KEY=your_secret_key_here

# 政务网门户登录凭证
PORTAL_USERNAME=your_username_here
PORTAL_PASSWORD=your_password_here

# DeepSeek API (可选)
DEEPSEEK_API_KEY=your_deepseek_key_here

# 数据库配置 (可选)
DATABASE_URL=sqlite:///data/market_supervision.db

# 日志级别
LOG_LEVEL=INFO

# Flask配置
FLASK_ENV=development
FLASK_DEBUG=False
SECRET_KEY=your_secret_key_here
